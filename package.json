{
  "name": "aframe-mirror-component",
  "version": "1.0.1",
  "description": "Mirror effect material that works with snapshots of the scene.",
  "main": "index.js",
  "cdn": "dist/aframe-mirror-component.min.js",
  "scripts": {
    "dev": "budo index.js:dist/aframe-mirror-component.min.js --port 7000 --live --open",
    "dist": "webpack index.js dist/aframe-mirror-component.js && webpack -p index.js dist/aframe-mirror-component.min.js",
    "lint": "semistandard -v | snazzy",
    "prepublish": "npm run dist",
    "ghpages": "ghpages",
    "start": "npm run dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alfa256/aframe-mirror-component.git"
  },
  "keywords": [
    "aframe",
    "aframe-component",
    "aframe-vr",
    "vr",
    "mozvr",
    "webvr",
    "mirror"
  ],
  "author": "Alfredo Consebola <sistemas.alfredo@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alfa256/aframe-mirror-component/issues"
  },
  "homepage": "https://github.com/alfa256/aframe-mirror-component#readme",
  "devDependencies": {
    "aframe": "*",
    "browserify": "^13.0.0",
    "budo": "^8.2.2",
    "chai": "^3.4.1",
    "chai-shallow-deep-equal": "^1.3.0",
    "ghpages": "^0.0.8",
    "karma": "^0.13.15",
    "karma-browserify": "^4.4.2",
    "karma-chai-shallow-deep-equal": "0.0.4",
    "karma-chrome-launcher": "2.0.0",
    "karma-env-preprocessor": "^0.1.1",
    "karma-firefox-launcher": "^0.1.7",
    "karma-mocha": "^0.2.1",
    "karma-mocha-reporter": "^1.1.3",
    "karma-sinon-chai": "^1.1.0",
    "mocha": "^2.3.4",
    "randomcolor": "^0.4.4",
    "semistandard": "^8.0.0",
    "shelljs": "^0.7.0",
    "sinon": "^1.17.5",
    "sinon-chai": "^2.8.0",
    "shx": "^0.1.1",
    "snazzy": "^4.0.0",
    "webpack": "^1.13.0"
  },
  "semistandard": {
    "globals": [
      "AFRAME",
      "THREE"
    ],
    "ignore": [
      "examples/build.js",
      "dist/**"
    ]
  }
}
